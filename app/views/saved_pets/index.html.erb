<p id="notice"><%= notice %></p>

<h1>fdsfsd Saved Pets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
     <% @saved_pets.where(:user => current_user) do |saved_pet| %>
      <tr>
        <td><%= User.find(current_user).email %></td>
        <td><%= Pet.find(pet_id).pet_name %></td>

        <br>
        <br>
        <td><%= link_to 'Show', saved_pet %></td>
        <td><%= link_to 'Edit', edit_saved_pet_path(saved_pet) %></td>
        <td><%= link_to 'Destroy', saved_pet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

