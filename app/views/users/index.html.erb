<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table class = "table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

    <tr>
      <th>Email</th>
    </tr>
  <tbody>
      <% @users.each do |user| %>
      <tr>
          <td><%= user.email %></td>

      </tr>
    <% end %>
  </tbody>
</table>

